  address = {New York, NY, USA},
  volume = {1},
  number = {1},
  issn = {0362-5915},
  url = {https://doi.org/10.1145/320434.320440},
  doi = {10.1145/320434.320440},
  abstract = {A data model, called the entity-relationship model, is proposed. This model incorporates
  some of the important semantic information about the real world. A special diagrammatic
  technique is introduced as a tool for database design. An example of database design
  and description using the model and the diagrammatic technique is given. Some implications
  for data integrity, information retrieval, and data manipulation are discussed.The
  entity-relationship model can be used as a basis for unification of different views
  of data: the network model, the relational model, and the entity set model. Semantic
  ambiguities in these models are analyzed. Possible ways to derive their views of data
  from the entity-relationship model are presented.},
  journal = {ACM Trans. Database Syst.},
  month = mar,
  pages = {9–36},
  numpages = {28},
  keywords = {semantics of data, logigcal view of data, data definition and manipulation, Data Base Task Group, entity set model, database design, relational model, data models, network model, data integrity and consistency, entity-relationship model}
}

